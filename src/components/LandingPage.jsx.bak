import PageLayout from "@/components/PageLayout";
import { defaultContent } from "@/data/pagesData";

// Now accepts a "data" prop
const LandingPage = ({ data }) => {
  // Logic: Use the data we passed in. If none passed, use default.
  const contentToRender = data || defaultContent;

  return <PageLayout content={contentToRender} />;
};

export default LandingPage;